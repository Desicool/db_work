<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ecust.db_work.entity.OrderDelivery" table="order-delivery" schema="db_work">
        <id name="orderId">
            <column name="orderID" sql-type="char(10)" length="10"/>
        </id>
        <property name="deliveryId">
            <column name="deliveryID" sql-type="char(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>