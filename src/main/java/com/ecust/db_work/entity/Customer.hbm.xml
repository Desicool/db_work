<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ecust.db_work.entity.Customer" table="customer" schema="db_work">
        <id name="customerId">
            <column name="customerID" sql-type="char(10)" length="10"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="sex">
            <column name="sex" sql-type="varchar(2)" length="2"/>
        </property>
        <list name="commonaddressesByCustomerId" inverse="true">
            <key>
                <column name="customerID"/>
            </key>
            <one-to-many not-found="ignore" class="com.ecust.db_work.entity.Commonaddress"/>
        </list>
        <list name="expressordersByCustomerId" inverse="true">
            <key>
                <column name="customerID"/>
            </key>
            <one-to-many not-found="ignore" class="com.ecust.db_work.entity.Expressorder"/>
        </list>
    </class>
</hibernate-mapping>