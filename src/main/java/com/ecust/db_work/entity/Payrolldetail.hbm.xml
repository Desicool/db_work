<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ecust.db_work.entity.Payrolldetail" table="payrolldetail" schema="db_work">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="payrollId">
            <column name="payrollID" sql-type="char(10)" length="10"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="money">
            <column name="money" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>